<h1><%= @category.name %></h1>
<ul>
  <% @items.each do |item| %>
    <li>
      <%= link_to item.name, item_path(item) %>

    </li>
  <% end %>
</ul>
<%= link_to "New Item", new_item_path %>
<%= button_to "Start Comparison", compare_category_path(@category), method: :get %>
<%= button_to "Delete category", category_path(@category), method: :delete %>
